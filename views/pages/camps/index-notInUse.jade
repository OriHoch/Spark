extends ../../includes/page
block content
    .camps.camp_index
        section.jumbotron
            .container
                h1.jumbotron-heading=t('camps:dash.title')
                p.lead.text-muted=t('camps:dash.desc')
                div.block-center
                    div.input-group
                        if userIs('admin')
                            a.Btn.Btn__default.reveal_create_camp_btn(href='javascript:;', role='button')=t('camps:new.btn_create')
                        if userIs('camp_manager')
                            a.Btn.Btn__default.reveal_manage_camp_btn(href='javascript:;', role='button')=t('camps:manage.manage_a_camp')
                        if !userIs('camp_manager') && user.isCampFree
                            a.Btn.Btn__default.reveal_join_camp_btn(href='javascript:;', role='button')=t('camps:join.header_title')
                .cards--wrapper
                    .choose_name.card.card-first.card-hide
                        if language === 'he'
                            button.card--close--he.Btn.Btn__close
                                 span.glyphicon.glyphicon-remove(aria-hidden='true')
                        else
                            .choose_name.card.card-first.card__padding--even.card-hide
                                button.card--close.Btn.Btn__close
                                     span.glyphicon.glyphicon-remove(aria-hidden='true')

                        h2=t('camps:new.header_title')
                        h4=t('camps:new.name_choosing_title')
                        .col-sm-8.col-xs-12
                            .pos-r
                                label=t('camps:new.name_en')
                                input(type="text", class="form-control", name="camp_name_en", autofocus="true", required)
                                span.indicator
                                    span.glyphicon
                        .col-sm-4.col-xs-12
                            a.Btn.Btn__green.Btn__inline(id="check_camp_name", role='button')=t('camps:btn_continue')

                    .join_camp.card.card-second.card-hide
                        if language === 'he'
                            button.card--close--he.Btn.Btn__close
                                 span.glyphicon.glyphicon-remove(aria-hidden='true')
                        else
                            button.card--close.Btn.Btn__close
                                 span.glyphicon.glyphicon-remove(aria-hidden='true')

                        h2=t('camps:join.header_title')
                        h4=t('camps:join.camp_choosing_title')
                        .col-sm-8.col-xs-12
                            label=t('camps:new.name_en')
                            input.hidden(id='join_camp_request_join_user_id', type='hidden', value='#{user.attributes.user_id}')
                            select(type="text", class="form-control", name="camp_name_en", autofocus="true", required)
                        .col-sm-4.col-xs-12
                            a.Btn.Btn__green.Btn__inline(id="join_camp_request_join_btn", role='button')=t('camps:join.request')

                    .camp_manage.card.card-third.card__padding--even.card-hide
                        if language === 'he'
                            button.card--close--he.Btn.Btn__close
                                 span.glyphicon.glyphicon-remove(aria-hidden='true')
                        else
                            button.card--close.Btn.Btn__close
                                 span.glyphicon.glyphicon-remove(aria-hidden='true')
                        .row.camp__manage--options
                            .col-sm-10
                                a.Btn.Btn__card(href='/#{language}/camps-stats', role='button')
                                    img(class="img-responsive #{language}", src="data:image/svg+xml;utf8;base64,")
                                    span(class="title_#{language}")=t('camps:dash.stats.title')
                                    span(class="arrow_#{language}")=t('camps:dash.arrow')
                            .col-sm-10
                                a.Btn.Btn__card(href='/#{language}/camps-members', role='button')
                                    img(class="img-responsive #{language}", src="data:image/svg+xml;utf8;base64,")
                                    span(class="title_#{language}")=t('camps:dash.members.title')
                                    span(class="arrow_#{language}")=t('camps:dash.arrow')
                            .col-sm-10
                                a.Btn.Btn__card(href='/#{language}/camps-docs', role='button')
                                    img(class="img-responsive #{language}", src="data:image/svg+xml;utf8;base64,")
                                    span(class="title_#{language}")=t('camps:dash.docs.title')
                                    span(class="arrow_#{language}")=t('camps:dash.arrow')
                            //- .col-sm-10
                            //-     a.Btn.Btn__card(href='/#{language}/program', role='button')
                            //-         img(class="img-responsive #{language}", src="data:image/svg+xml;utf8;base64,")
                            //-         span(class="title_#{language}")=t('camps:dash.program.title')
                            //-         span(class="arrow_#{language}")=t('camps:dash.arrow')

                //- Camp join pending request card
                if user.isCampJoinPending
                    .card.card__padding--even.block-center
                        h3 You are waiting camp manager approval for:
                        p
                            u Tree of Light
                        p requested at 25-02-2017 

        // Camp join request modal
        .modal.fade(id='join_camp_request_modal', tabindex='-1', role='dialog')
            .modal-dialog.card.card__padding--even.card__shad(role='document')
                div
                    .modal-header
                        button.close(type='button', data-dismiss='modal', aria-label='Close')
                            span(aria-hidden='true') ×
                        h4.modal-title=t('camps:dash.join_request')
                    .modal-body
                        p=t('camps:dash.join_request_desc')
                        .user_details
                    .modal-footer
                        button.Btn.Btn__transparent(id='join_camp_close_btn', type='button', data-dismiss='modal')=t('camps:dash.join_close')
                        button.Btn.Btn__green(id='join_camp_send_request_btn', type='button')=t('camps:dash.join_send')
